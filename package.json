{
  "name": "maoragai",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "cd ${pwd} && husky install ${pwd}/.husky",
    "lint": "npx nx lint --verbose",
    "affected:lint": "npx nx affected --target=lint --verbose",
    "test": "npx nx test --verbose",
    "affected:test": "npx nx affected --target=test --verbose",
    "build:dev": "npx nx build --args='--envType=dev' --verbose",
    "build:test": "npx nx build --args='--envType=test' --verbose",
    "build:prod": "npx nx build --args='--envType=prod' --verbose",
    "affected:build:dev": "npx nx affected --target=build --args='--envType=dev' --verbose",
    "affected:build:test": "npx nx affected --target=build --args='--envType=test' --verbose",
    "affected:build:prod": "npx nx affected --target=build --args='--envType=prod' --verbose",
    "deploy:dev": "npx nx deploy --args='--envType=dev' --verbose",
    "deploy:test": "npx nx deploy --args='--envType=test' --verbose",
    "deploy:prod": "npx nx deploy --args='--envType=prod' --verbose",
    "affected:deploy:dev": "npx nx affected --target=deploy --args='--envType=dev' --verbose",
    "affected:deploy:test": "npx nx affected --target=deploy --args='--envType=test' --verbose",
    "affected:deploy:prod": "npx nx affected --target=deploy --args='--envType=prod' --verbose"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/jest": "^14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/node": "^14.7.5",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.7.5",
    "@types/aws-lambda": "^8.10.104",
    "@types/jest": "^29.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.36.1",
    "copy-partial-json": "^1.1.4",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "nx": "14.7.5",
    "ts-jest": "^29.0.1",
    "ts-node": "10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "workspaces": [
    "packages/**"
  ]
}
